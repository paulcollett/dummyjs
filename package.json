{
  "name": "dummyjs",
  "version": "1.1.1",
  "description": "placeholder text & images for HTML prototyping",
  "main": "dist/dummy.cjs.js",
  "module": "dist/dummy.es2015.js",
  "unpkg": "dist/dummy.min.js",
  "jsdelivr": "dist/dummy.min.js",
  "author": "Paul Collett",
  "license": "MIT",
  "scripts": {
    "build": "node ./build.js",
